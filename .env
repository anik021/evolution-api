# Chave de autenticação fictícia para teste local
AUTHENTICATION_API_KEY=test123456

# Configuração do banco de dados PostgreSQL
POSTGRES_DATABASE=evolution_db
POSTGRES_USERNAME=postgres
POSTGRES_PASSWORD=postgres
DATABASE_PROVIDER=postgresql

# URL de conexão do Prisma (aponta para o container do postgres no docker-compose)
DATABASE_URL=postgresql://postgres:postgres@evolution-postgres:5432/evolution_db

# Outras variáveis que podem ser necessárias
NODE_ENV=development
PORT=8080
REDIS_HOST=redis
REDIS_PORT=6379
